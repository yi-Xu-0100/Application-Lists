{
  "name": "application-lists",
  "version": "1.0.0",
  "description": "personal application lists.",
  "scripts": {
    "prepare": "husky install",
    "lint-check": "prettier --check **/*.{js,yml,yaml,json,md}",
    "format": "prettier --write **/*.{js,yml,yaml,json,md}",
    "clear": "cd ./docs/ && shx rm -rf _sidebar.md config.md index.html LICENSE README.md && cd ../",
    "copy": "shx cp .docsify/* ./docs/ && shx cp ./config/README.md ./docs/config.md && shx cp README.md ./docs/README.md && shx cp LICENSE ./docs/LICENSE",
    "start": "npm run copy && docsify serve docs"
  },
  "lint-staged": {
    "**/*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yi-Xu-0100/Application-Lists.git"
  },
  "keywords": [
    "windows",
    "application",
    "reinstallation"
  ],
  "author": "yi_Xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yi-Xu-0100/Application-Lists/issues"
  },
  "homepage": "https://github.com/yi-Xu-0100/Application-Lists#readme",
  "devDependencies": {
    "docsify-cli": "^4.4.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "marked": "^2.0.4",
    "prettier": "2.3.0",
    "shx": "^0.3.2"
  }
}
